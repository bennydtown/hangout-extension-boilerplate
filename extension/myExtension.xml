<?xml version="1.0" encoding="UTF-8" ?>
<Module>
  <ModulePrefs title="Poker (Extension)">
          <Require feature="rpc" />
          <Require feature="views" />
          <Require feature="locked-domain" />
  </ModulePrefs>
  <Content type="html"><![CDATA[
    <html>
      <head>
        <link type="text/css" rel="stylesheet" href="https://raw.githubusercontent.com/bennydtown/hangout-extension-boilerplate/master/extension/static/css/tag.css" />
        <link type="text/css" rel="stylesheet" href="https://raw.githubusercontent.com/bennydtown/hangout-extension-boilerplate/master/extension/static/css/log.css" />
        <script
          data-main="https://raw.githubusercontent.com/bennydtown/hangout-extension-boilerplate/master/extension/static/js/init"
          src="https://raw.githubusercontent.com/bennydtown/hangout-extension-boilerplate/master/extension/static/js/lib/require-2.1.15-min.js"
        data-cover></script>
        <script type="text/javascript" src="//plus.google.com/hangouts/_/api/v1/hangout.js"></script>
        <script type="text/javascript">
          var hangoutApiReady = false;
          gadgets.util.registerOnLoadHandler(function(){
            hangoutApiReady = true;
          });
        </script>
      </head>
      <body>

        <div id="extensionWrapper">

          <h3>Hangout Extension Boilerplate</h3>

          <div id="log"></div>

          <div id="gameControls"></div>

        </div>

      </body>
    </html>
  ]]></Content>
</Module>
